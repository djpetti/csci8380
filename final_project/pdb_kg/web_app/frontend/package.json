{
  "name": "pdb-kg-frontend",
  "version": "0.1.0",
  "description": "PDB-KG frontend application",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/*.ts' 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/**.ts src/**/*.ts --write",
    "bundle": "rollup -c",
    "api": "rm -rf gen/api && openapi-generator-cli generate -i http://127.0.0.1:8000/openapi.json --generator-name typescript-axios -o gen/api --config api.json && cd gen/api && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djpetti/csci8380.git"
  },
  "keywords": [
    "semantic_web"
  ],
  "author": "Daniel Petti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djpetti/csci8380/issues"
  },
  "homepage": "https://github.com/djpetti/csci8380#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "@openapitools/openapi-generator-cli": "^2.4.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/jest-each": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "faker": "^5.4.0",
    "jest-each": "^26.6.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.2.1",
    "rollup": "^2.38.5",
    "rollup-jest": "^1.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.1",
    "ts-jest": "^26.5.2",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-icon-button": "^0.25.3",
    "@material/mwc-linear-progress": "^0.25.3",
    "@material/mwc-list": "^0.25.3",
    "@material/mwc-textfield": "^0.25.3",
    "@material/mwc-top-app-bar-fixed": "^0.25.3",
    "graphology": "^0.23.1",
    "graphology-layout": "^0.4.1",
    "graphology-layout-force": "^0.2.3",
    "lit": "^2.0.2",
    "materialize-css": "^1.0.0-rc.2",
    "sigma": "^2.0.0",
    "typescript-axios": "file:gen/api"
  }
}
